                <subItem>
                    <className>CPdbDataFile</className>
                    <qualifiers>
                        <mustExist>True</mustExist>
                        <allowUndefined>True</allowUndefined>
                    </qualifiers>
                </subItem>
            </content>
            <content id="FPHIIN">
                <className>CList</className>
                <qualifiers>
                </qualifiers>
                <subItem>
                    <className>CMapCoeffsDataFile</className>
                    <qualifiers>
                        <requiredSubType>1</requiredSubType>
                        <allowUndefined>True</allowUndefined>
                    </qualifiers>
                </subItem>
            </content>
            <content id="DELFPHIIN">
                <className>CList</className>
                <qualifiers>
                </qualifiers>
                <subItem>
                    <className>CMapCoeffsDataFile</className>
                    <qualifiers>
                        <requiredSubType>2</requiredSubType>
                        <allowUndefined>True</allowUndefined>
                    </qualifiers>
                </subItem>
            </content>
            <content id="MAPIN">
                <className>CList</className>
                <qualifiers>
                </qualifiers>
                <subItem>
                    <className>CMapDataFile</className>
                    <qualifiers>
                        <requiredSubType>1</requiredSubType>
                        <allowUndefined>True</allowUndefined>
                    </qualifiers>
                </subItem>
            </content>            
            <content id="DICTIN">
                <className>CList</className>
                <qualifiers>
                </qualifiers>
                <subItem>
                    <className>CDictDataFile</className>
                    <qualifiers>
                        <allowUndefined>True</allowUndefined>
                    </qualifiers>
                </subItem>
            </content>            
        </container>
        <container id="controlParameters">
            <content id="STARTPOINT">
                <className>CString</className>
                <qualifiers>
                <onlyEnumerators>True</onlyEnumerators>
                <enumerators>FIT_LIGAND</enumerators>
                <menuText>Fit ligand</menuText>
                <label>Code snippet to start from</label>
                <default>BLANK</default>
                </qualifiers>
            </content>
        </container>
        <container id="outputData">
